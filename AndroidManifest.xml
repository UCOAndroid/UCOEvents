<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="edu.uco" android:versionCode="1" android:versionName="1.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:debuggable="true">
		<activity android:name=".HelloEventsWidget" android:label="@string/app_name"
			android:theme="@android:style/Theme.NoTitleBar">

		</activity>

		<activity android:name=".TodayActivity" android:label="@string/app_name">
		</activity>
		<activity android:name=".WeekActivity" android:label="@string/app_name">
		</activity>
		<activity android:name=".MonthActivity" android:label="@string/app_name">
		</activity>
		<activity android:name=".NotifyManager" android:label="@string/app_name">
			<intent-filter>
				<category android:name="android.intent.category.DEFAULT" />
				<action android:name="com.google.app.splashy.CLEARSPLASH" />
			</intent-filter>
		</activity>
		<activity android:name=".AboutActivity" android:label="@string/app_name">
		</activity>
		<activity android:name=".EventListActivity" android:label="@string/app_name">
		</activity>
		<activity android:name=".SplashActivity" android:label="@string/app_name">
			<intent-filter>
				<category android:name="android.intent.category.LAUNCHER" />
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
	</application>


</manifest> 