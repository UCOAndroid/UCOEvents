<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/layout_root" android:orientation="vertical"
	android:layout_width="275dp" android:layout_height="200dp">
	<TextView android:id="@+id/date" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="#FFF" />
	<TextView android:id="@+id/location" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="#FFF" />
	<TextView android:id="@+id/cost" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="#FFF" />
	<TextView android:id="@+id/sponsor" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="#FFF" />
	<TextView android:id="@+id/eventdescription"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:textColor="#FFF" />
	<Button android:id="@+id/ImageCancelButton"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:text="OK" />
</LinearLayout>